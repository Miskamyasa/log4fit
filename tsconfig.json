  "compilerOptions": {
    "downlevelIteration": true,
    "alwaysStrict": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "allowJs": true,
    "checkJs": false,
    "removeComments": true,
    "types": []
  }
}
