{
  "private": false,
  "name": "log4fit",
  "url": "https://log4fit.com",
  "version": "1.0.27",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.12.0"
  },
  "scripts": {
    "build:all": "./version_bump && eas build --profile production --platform all --local && eas update --channel production --platform all --auto",
    "build:android": "./version_bump && eas build --profile production --platform android --local && eas update --channel production --platform android --auto",
    "build:android:dev": "eas build --profile development --platform android --local",
    "build:ios": "./version_bump && eas build --profile production --platform ios --local && eas update --channel production --platform ios --auto",
    "build:ios:dev": "eas build --profile development-simulator --platform ios --local",
    "dev": "expo start --dev-client",
    "lint": "tsc --noEmit && eslint ./src --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@expo/vector-icons": "13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@redux-devtools/extension": "3.2.6",
    "@testing-library/react-hooks": "8.0.1",
    "@types/i18n-js": "3.8.8",
    "@types/lodash": "4.14.201",
    "@types/node": "16.18.11",
    "@types/react": "18.2.37",
    "@types/react-native": "0.71.3",
    "@types/react-redux": "7.1.30",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.52.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "expo": "49.0.16",
    "expo-application": "5.3.1",
    "expo-asset": "8.10.1",
    "expo-build-properties": "0.8.3",
    "expo-constants": "14.4.2",
    "expo-dev-client": "2.4.11",
    "expo-device": "5.4.0",
    "expo-file-system": "15.4.4",
    "expo-image": "1.3.4",
    "expo-linking": "5.0.2",
    "expo-localization": "14.3.0",
    "expo-random": "13.2.0",
    "expo-splash-screen": "0.20.5",
    "expo-status-bar": "1.6.0",
    "expo-updates": "0.18.17",
    "husky": "8.0.3",
    "i18n-js": "3.8.0",
    "jest": "29.5.0",
    "jest-expo": "49.0.0",
    "lodash": "4.17.21",
    "metro-core": "0.76.1",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-dialog": "9.2.0",
    "react-native-dotenv": "3.4.8",
    "react-native-get-random-values": "1.9.0",
    "react-native-modal": "13.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "3.22.1",
    "react-redux": "8.0.5",
    "react-test-renderer": "18.2.0",
    "redux": "4.2.1",
