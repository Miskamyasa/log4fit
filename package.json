{
  "private": false,
  "name": "log4fit",
  "url": "https://log4fit.com",
  "version": "1.0.43",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.6.5"
  },
  "scripts": {
    "build:all": "./version_bump && eas build --profile production --platform all --local",
    "build:android": "./version_bump && eas build --profile production --platform android --local",
    "build:android:dev": "eas build --profile development --platform android --local",
    "build:ios": "./version_bump && eas build --profile production --platform ios --local",
    "build:ios:dev": "eas build --profile development-simulator --platform ios --local",
    "build:ios:device": "eas build --profile development --platform ios --local",
    "dev": "expo start --dev-client",
    "lint": "tsc --noEmit && eslint ./src --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "publish:all": "eas update --channel production --platform all --auto",
    "publish:android": "eas update --channel production --platform android --auto",
    "publish:ios": "eas update --channel production --platform ios --auto",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.23.3",
    "@expo/vector-icons": "14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@redux-devtools/extension": "3.2.6",
    "@shopify/flash-list": "1.6.4",
    "@testing-library/react-hooks": "8.0.1",
    "@types/i18n-js": "3.8.8",
    "@types/lodash": "4.14.201",
    "@types/node": "16.18.11",
    "@types/react": "18.2.79",
    "@types/react-redux": "7.1.30",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.52.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "expo": "51.0.24",
    "expo-application": "5.9.1",
    "expo-asset": "10.0.10",
    "expo-build-properties": "0.12.4",
    "expo-constants": "16.0.2",
    "expo-dev-client": "4.0.21",
    "expo-device": "6.0.2",
    "expo-file-system": "17.0.1",
    "expo-image": "1.12.13",
    "expo-linking": "6.3.1",
    "expo-localization": "15.0.3",
    "expo-random": "14.0.1",
    "expo-splash-screen": "0.27.5",
    "expo-status-bar": "1.12.1",
    "expo-updates": "0.25.21",
    "husky": "8.0.3",
    "i18n-js": "3.8.0",
    "jest": "29.5.0",
    "jest-expo": "51.0.3",
    "lodash": "4.17.21",
    "metro-core": "0.76.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-get-random-values": "1.11.0",
    "react-native-modal": "13.0.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-redux": "8.0.5",
    "react-test-renderer": "18.2.0",
    "redux": "4.2.1",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.3",
    "typescript": "5.3.3",
