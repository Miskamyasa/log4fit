{
    "expo": {
        "name": "Log4Fit",
        "slug": "log4fit",
        "version": "1.0.27",
        "runtimeVersion": {
            "policy": "sdkVersion"
        },
        "orientation": "portrait",
        "scheme": "log4fit",
        "icon": "./assets/images/1024.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "assets/images/*",
            "src/assets/images/*"
        ],
        "ios": {
            "buildNumber": "37",
            "supportsTablet": false,
            "requireFullScreen": true,
            "bundleIdentifier": "red.paragraph.log4fit",
            "infoPlist": {
                "CFBundleAllowMixedLocalizations": true
            }
        },
        "android": {
            "versionCode": 37,
            "package": "red.paragraph.log4fit",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive_icon.png",
                "backgroundColor": "#F6F6F6"
            }
        },
        "locales": {
            "ru": "./locales/russian.json",
            "en": "./locales/english.json"
        },
        "extra": {
            "eas": {
                "projectId": "213ef32b-98a5-47e3-91da-79de2e23283b"
            }
        },
        "updates": {
            "url": "https://u.expo.dev/213ef32b-98a5-47e3-91da-79de2e23283b"
        },
        "plugins": [
            [
                "expo-build-properties",
                {
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ],
            "expo-localization",
            "./wass.js"
        ]
    }
}
