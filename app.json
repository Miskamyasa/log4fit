{
  "expo": {
    "name": "Log4Fit",
    "slug": "log4fit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "log4fit",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "red.paragraph.log4fit",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "versionCode": 11,
      "package": "red.paragraph.log4fit",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/custom.png",
        "backgroundColor": "#FFFFFF"
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "locales": {
      "ru": "./locales/russian.json",
      "en": "./locales/english.json"
    },
    "extra": {
      "eas": {
        "projectId": "213ef32b-98a5-47e3-91da-79de2e23283b"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/213ef32b-98a5-47e3-91da-79de2e23283b"
    }
  }
