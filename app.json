{
  "expo": {
    "name": "Log4Fit",
    "slug": "log4fit",
    "version": "1.0.8",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "scheme": "log4fit",
    "icon": "./assets/images/1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "src/assets/images/*"
    ],
    "ios": {
      "buildNumber": "18",
      "supportsTablet": false,
      "requireFullScreen": true,
      "bundleIdentifier": "red.paragraph.log4fit",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 18,
      "package": "red.paragraph.log4fit",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1024.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "locales": {
      "ru": "./locales/russian.json",
      "en": "./locales/english.json"
    },
    "extra": {
      "eas": {
        "projectId": "213ef32b-98a5-47e3-91da-79de2e23283b"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/213ef32b-98a5-47e3-91da-79de2e23283b"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "sentry-expo",
      "@react-native-firebase/app",
      [
        "./wass.js",
        {
          "resizeable": false,
          "smallScreens": false,
          "normalScreens": true,
          "largeScreens": true,
          "xlargeScreens": false,
          "anyDensity": true,
          "requiresSmallestWidthDp": 320,
          "compatibleWidthLimitDp": 480,
          "largestWidthLimitDp": 600
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "miskamyasa",
            "project": "log4fit",
            "authToken": "1568e9f17c084ac5a3980f0ad9a182e4933e9b83838045bfa06d0a772a338e7d"
          }
        }
      ]
    }
  }
}
