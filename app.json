{
  "expo": {
    "name": "Log4Fit",
    "slug": "log4fit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "log4fit",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "red.paragraph.log4fit",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 11,
      "package": "red.paragraph.log4fit",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/custom.png",
        "backgroundColor": "#FFFFFF"
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "locales": {
      "ru": "./locales/russian.json",
      "en": "./locales/english.json"
    },
    "extra": {
      "eas": {
        "projectId": "213ef32b-98a5-47e3-91da-79de2e23283b"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/213ef32b-98a5-47e3-91da-79de2e23283b"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "sentry-expo",
      "@react-native-firebase/app",
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "miskamyasa",
            "project": "log4fit",
            "authToken": "1568e9f17c084ac5a3980f0ad9a182e4933e9b83838045bfa06d0a772a338e7d"
          }
        }
      ]
    }
  }
}
